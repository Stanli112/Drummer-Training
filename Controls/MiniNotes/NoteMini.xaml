<UserControl x:Class="CoordinationTraining.Controls.MiniNotes.NoteMini"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CoordinationTraining.Controls.MiniNotes"
             mc:Ignorable="d" 
             MaxHeight="25" MaxWidth="10"
             MinHeight="25" MinWidth="10">
    <Grid>
        <Rectangle Width="2" HorizontalAlignment="Center" Fill="Black"
                   Margin="0,4,0,6"/>
        <Ellipse Width="10" Height="6" Fill="Black">
            <Ellipse.RenderTransform>
                <RotateTransform Angle="-15" CenterX="30" CenterY="21"/>
            </Ellipse.RenderTransform>
        </Ellipse>
    </Grid>
</UserControl>
