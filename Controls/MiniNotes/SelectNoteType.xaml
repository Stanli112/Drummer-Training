<Window x:Class="CoordinationTraining.Controls.MiniNotes.SelectNoteType"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CoordinationTraining.Controls.MiniNotes"
        mc:Ignorable="d"
        Title="SelectNoteType" Height="200" Width="150"
        WindowStyle="SingleBorderWindow" Background="#969696"
        WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize">
    <Grid>
        <ListBox x:Name="lbNotes" Background="Transparent" 
                 MouseDoubleClick="lbNotes_MouseDoubleClick" 
                 BorderThickness="0" HorizontalContentAlignment="Center" ToolTip="Double click -> select a note">
            <local:Fourth_mini/>
            <local:Eight_mini/>
            <local:Threeol_mini/>
            <local:Sixteenth_mini/>
            <local:ThirtySecond_mini/>

        </ListBox>
    </Grid>
</Window>
